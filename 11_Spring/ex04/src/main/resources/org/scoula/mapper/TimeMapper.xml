<?xml version="1.0" encoding="UTF-8" ?>
<!--DTD : 문서 타입 정의-->
<!--MyBatis 구성파일이 유효한지 검사-->
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.scoula.mapper.TimeMapper">
<!--    해당 sql문을 getTime2 함수에 매핑시켜줌-->
<!--    SELECT문의 경우 resultType이 필수로 들어가야한다(return할 데이터 타입)-->
<!--    쿼리가 길어질 경우 SQL문과 메소드가 섞이기 때문에 인터페이스 분리-->
    <select id="getTime2" resultType="java.lang.String">
        SELECT SYSDATE()
    </select>
</mapper>
